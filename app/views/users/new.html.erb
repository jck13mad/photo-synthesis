<h1>Sign Up Here</h1>
<br />
<%= render partial: '/shared/errors', locals: {obj: @user }%>
<br />
<%= button_to "Log in with Google", '/auth/google_oauth2' %>
<br />
<%= form_with(model: @user, url:'/signup', local: true) do |f|%>
    <%= f.label :username %>
    <%= f.text_field :username %><br><br>
    <%= f.label :email %>
    <%= f.email_field :email %><br><br>
    <%= f.label :password %>
    <%= f.password_field :password %><br><br>
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %><br><br>
    <%= f.submit "Sign Up"%>
<% end %>